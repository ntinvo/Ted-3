{
  "name": "teddy",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "client": "cd client && npm run start",
    "server": "nodemon --watch server --exec babel-node -- ./server/bin/www",
    "start": "concurrently \"npm run build:watch\" \"npm run server\"",
    "startdev": "concurrently \"npm run build:watch\" \"npm run serve\" \"npm run server\"",
    "build": "cd client && npm run build",
    "serve": "lite-server -c=bs-config.json",
    "build:watch": "cd client && npm run build:watch",
    "build:e2e": "cd client && npm run build:e2e",
    "installclient": "cd client && npm install",
    "setup": "concurrently \"yarn install\" \"cd client & yarn install\"",
    "webpack": "webpack --config webpack.config.js",
    "webpack:prod": "webpack -p --config webpack.config.js",
    "webpack:err": "webpack --display-error-details",
    "webpack:clean": "rm -rf client/dist",
    "webpack:ver": "webpack --version"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "knex": "^0.12.9",
    "morgan": "~1.8.1",
    "pg": "^6.1.5",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "concurrently": "^3.4.0",
    "json-loader": "^0.5.4",
    "lite-server": "^2.2.2",
    "ts-loader": "^2.0.3",
    "typescript": "^2.1.6",
    "webpack": "^2.3.3"
  }
}
